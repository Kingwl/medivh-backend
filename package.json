{
  "name": "medivh-backend",
  "version": "1.0.0",
  "engines": {
    "node": "6.3.0"
  },
  "description": "Medivh-backend是一个轻量级博客的服务端部分",
  "main": "index.js",
  "bin": {
    "medivh-backend": "development.js"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development node bin/development.js",
    "test": "cross-env NODE_ENV=test node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- --recursive && node bin/test.js",
    "ci": "cross-env NODE_ENV=ci node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kingwl/Medivh-backend.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kingwl/Medivh-backend/issues"
  },
  "homepage": "https://github.com/Kingwl/Medivh-backend#readme",
  "dependencies": {
    "bluebird": "^3.4.1",
    "cross-env": "^2.0.0",
    "koa": "^1.2.0",
    "koa-bodyparser": "^2.2.0",
    "koa-json": "^1.1.3",
    "koa-logger": "^1.3.0",
    "koa-router": "^5.4.0",
    "mongoose": "^4.5.4",
    "node-monkey": "^0.2.16"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chokidar": "^1.6.0",
    "co-mocha": "^1.1.2",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3",
    "opener": "^1.4.1",
    "request": "^2.73.0",
    "supertest": "^1.2.0",
    "supertest-koa-agent": "^0.2.1"
  }
